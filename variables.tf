variable "folder_id" {
  description = "Default folder ID in yandex cloud"
  type        = string
  default     = "FOLDER_ID"
}

variable "cloud_id" {
  description = "Default cloud ID in yandex cloud"
  type        = string
  default     = "CLOUD_ID"
}

variable "token" {
  description = "Default token in yandex cloud"
  type        = string
  default     = "TOKEN_YC"
}

variable "role_editor" {
  description = "Default role access edit in yandex cloud"
  type        = string
  default     = "ROLE_YC_AE"
}
